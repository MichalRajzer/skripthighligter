{
  "name": "skripthighligter",
  "displayName": "SkriptHighligter",
  "description": "Highlighter for the Skript language used by bukkit",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "skript",
      "aliases": ["Skript", "skript"],
      "extensions": [".sk"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "skript",
      "scopeName": "source.skript",
      "path": "./syntaxes/skript.tmLanguage.json"
    }]
  }
}
